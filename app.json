{
  "expo": {
    "name": "learn-expo-app",
    "slug": "learn-expo-app",
    "version": "1.0.0",
    "scheme": "learn-expo-app",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.halydev.learn.expo.app"
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ],
      "expo-web-browser",
      "expo-background-task"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0456146c-af84-4d5c-aa2c-33683921c9b9"
      }
    },
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "owner": "halydev",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/0456146c-af84-4d5c-aa2c-33683921c9b9"
    }
  }
}
